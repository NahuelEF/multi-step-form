{
  "jsxSingleQuote": true,
  "printWidth": 100,
  "singleQuote": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^[.]",
    "^@/components/(.*)$",
    "^@/validations/(.*)$",
    "^@/constants/(.*)$",
    "^[./]",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx"]
}
